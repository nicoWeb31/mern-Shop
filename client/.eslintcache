up\">\n                    <input\n                        type=\"email\"\n                        value={email}\n                        className=\"form-control\"\n                        onChange={(e) => setEmail(e.target.value)}\n                        placeholder=\"Type your email\"\n                        autoFocus\n                    />\n                </div>\n\n                <hr />\n                <button className=\"btn btn-raised\" disabled={!email}>\n                    {\" \"}\n                    Submit{\" \"}\n                </button>\n            </form>\n        </div>\n    );\n};\n\nexport default ForgotPassword;\n","/media/ni/新加卷/mern-stack/full-shop/client/src/function/auth.js",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/pages/user/History.jsx",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/components/routes/UserRoute.jsx",["126"],"import React from \"react\";\nimport { Route, Link } from \"react-router-dom\";\nimport { useSelector } from \"react-redux\";\nimport LoadingToRedirect from \"./LoadingToRedirect\";\n\nconst UserRoute = ({ children, ...rest }) => {\n    const user = useSelector((state) => state.auth);\n\n    \n\n    return user && user.token ? <Route {...rest} render={()=>children}/> : <LoadingToRedirect/> ;\n};\n\nexport default UserRoute;\n","/media/ni/新加卷/mern-stack/full-shop/client/src/components/routes/LoadingToRedirect.jsx",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/components/sidBarNav/UserNav.jsx",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/pages/user/WhishList.jsx",[],["127","128"],"/media/ni/新加卷/mern-stack/full-shop/client/src/pages/user/Password.jsx",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/components/routes/AdminRoute.jsx",["129"],{"ruleId":"130","severity":1,"message":"131","line":17,"column":10,"nodeType":"132","messageId":"133","endLine":17,"endColumn":21},{"ruleId":"134","severity":1,"message":"135","line":45,"column":8,"nodeType":"136","endLine":45,"endColumn":10,"suggestions":"137