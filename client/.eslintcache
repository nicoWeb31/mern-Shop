rm onSubmit={onhandleSubmit}>\n                <div className=\"form-group\">\n                    <label className=\"\">Name</label>\n                    <input\n                        className=\"form-control\"\n                        required\n                        autoFocus\n                        type=\"text\"\n                        value={updateCategory}\n                        onChange={(e) => setUpdateCategory(e.target.value)}\n                    />\n                    <button type=\"submit\" className=\"btn btn-outline-primary\">\n                        enoyer\n                    </button>\n                </div>\n            </form>\n        );\n    };\n\n    return (\n        <div className=\"container-fluid\">\n            <div className=\"row\">\n                <div className=\"col-md-2\">\n                    <AdminNav />\n                </div>\n                <div className=\"col\">{showCategoryForm()}</div>\n            </div>\n        </div>\n    );\n};\n\nexport default CategoryUpdate;\n","/media/ni/新加卷/mern-stack/full-shop/client/src/redux/reducers/categoryReducer.js",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/redux/reducers/index.js",[],["128","129"],"/media/ni/新加卷/mern-stack/full-shop/client/src/redux/store.js",[],["130","131"],"/media/ni/新加卷/mern-stack/full-shop/client/src/pages/user/Password.jsx",[],["132","133"],"/media/ni/新加卷/mern-stack/full-shop/client/src/pages/admin/AdminDashbord.jsx",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/pages/auth/registerComplete/RegisterComplete.jsx",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/function/auth.js",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/components/sidBarNav/UserNav.jsx",[],["134","135"],"/media/ni/新加卷/mern-stack/full-shop/client/src/function/category.js",[],"/media/ni/新加卷/mern-stack/full-shop/client/src/components/menu/Header.jsx",[],["136","137"],"/media/ni/新加卷/mern-stack/full-shop/client/src/redux/type/categoryType.js",[],"/media/ni/新加卷/mern-stack/full-